<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("component.log.SendModeLog")]
	</fx:Metadata>
	
	<s:VGroup>
		<s:HGroup>
			<s:Label id="preMonth" text="이전달" fontFamily="NGD" />
			<s:Label id="month" text="" fontSize="26" fontFamily="NGD" />
			<s:Label id="nextMonth" text="다음달" fontFamily="NGD" />
		</s:HGroup>
		<s:HSlider id="monthSlider" width="100%" />
		<s:List id="groupList"
				width="250"
				height="400"
				itemRenderer="skin.log.GroupListRenderer" />
	</s:VGroup>
	<s:RichText id="close" text="X" right="10" fontSize="26" fontFamily="NGD" />
	
</s:Skin>
