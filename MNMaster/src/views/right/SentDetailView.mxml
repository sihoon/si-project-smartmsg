<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:services="services.*"
		xmlns:valueObjects="valueObjects.*"
		title="SentDetailView">
	
	<fx:Script>
		<![CDATA[
			import comm.Popup;
			import comm.SLibrary;
			
			import mx.rpc.events.ResultEvent;
			
			import spark.components.SplitViewNavigator;
			import spark.components.ViewNavigator;
			
			import valueObjects.CommonVO;
			import valueObjects.SentLogVO;
			
			import views.left.MemberView;
			
			
		
			override public function set data(value:Object):void
			{
				// TODO Auto Generated method stub
				super.data = value;
				sentLogVO = super.data as SentLogVO;
				
			}
			
			
			protected function btnCharge_clickHandler(event:MouseEvent):void
			{
				//SLibrary.alert("update Success.");
				navigator.pushView(ChargeView, sentLogVO);
			}
			
			
			protected function button_clickHandler(event:MouseEvent):void
			{
				sentLogVO.idx = parseInt(idxTextInput.text);
				sentLogVO.method = methodTextInput.text;
				sentLogVO.cnt = parseInt(cntTextInput.text);
				sentLogVO.line = lineTextInput.text;
				sentLogVO.message = messageTextInput.text;
				sentLogVO.timeSend = timeSendTextInput.text;
				sentLogVO.user_ip = user_ipTextInput.text;
				sentLogVO.mode = modeTextInput.text;
				sentLogVO.rownum = parseInt(rownumTextInput.text);
				sentLogVO.timeWrite = timeWriteTextInput.text;
				sentLogVO.start = parseInt(startTextInput.text);
				sentLogVO.user_id = user_idTextInput.text;
				sentLogVO.end = parseInt(endTextInput.text);
				sentLogVO.timeDel = timeDelTextInput.text;
				sentLogVO.delType = delTypeTextInput.text;
				sentLogVO.ynDel = ynDelTextInput.text;
			}
			
		]]>
	</fx:Script>
	<fx:Declarations>
		<valueObjects:SentLogVO id="sentLogVO"/>
		
	</fx:Declarations>
	<s:actionContent>
		<s:Button id="btnCharge" label="detail" click="btnCharge_clickHandler(event)"/>
	</s:actionContent>
	
	<s:Scroller width="100%" height="100%">
		<s:VGroup>
			<s:Form>
				<s:FormItem label="Idx">
					<s:TextInput id="idxTextInput" text="{sentLogVO.idx}"/>
				</s:FormItem>
				<s:FormItem label="Method">
					<s:TextInput id="methodTextInput" text="{sentLogVO.method}"/>
				</s:FormItem>
				<s:FormItem label="Cnt">
					<s:TextInput id="cntTextInput" text="{sentLogVO.cnt}"/>
				</s:FormItem>
				<s:FormItem label="Line">
					<s:TextInput id="lineTextInput" text="{sentLogVO.line}"/>
				</s:FormItem>
				<s:FormItem label="Message">
					<s:TextArea id="messageTextInput" text="{sentLogVO.message}"/>
				</s:FormItem>
				<s:FormItem label="TimeSend">
					<s:TextInput id="timeSendTextInput" text="{sentLogVO.timeSend}"/>
				</s:FormItem>
				<s:FormItem label="User_ip">
					<s:TextInput id="user_ipTextInput" text="{sentLogVO.user_ip}"/>
				</s:FormItem>
				<s:FormItem label="Mode">
					<s:TextInput id="modeTextInput" text="{sentLogVO.mode}"/>
				</s:FormItem>
				<s:FormItem label="Rownum">
					<s:TextInput id="rownumTextInput" text="{sentLogVO.rownum}"/>
				</s:FormItem>
				<s:FormItem label="TimeWrite">
					<s:TextInput id="timeWriteTextInput" text="{sentLogVO.timeWrite}"/>
				</s:FormItem>
				<s:FormItem label="Start">
					<s:TextInput id="startTextInput" text="{sentLogVO.start}"/>
				</s:FormItem>
				<s:FormItem label="User_id">
					<s:TextInput id="user_idTextInput" text="{sentLogVO.user_id}"/>
				</s:FormItem>
				<s:FormItem label="End">
					<s:TextInput id="endTextInput" text="{sentLogVO.end}"/>
				</s:FormItem>
				<s:FormItem label="TimeDel">
					<s:TextInput id="timeDelTextInput" text="{sentLogVO.timeDel}"/>
				</s:FormItem>
				<s:FormItem label="DelType">
					<s:TextInput id="delTypeTextInput" text="{sentLogVO.delType}"/>
				</s:FormItem>
				<s:FormItem label="YnDel">
					<s:TextInput id="ynDelTextInput" text="{sentLogVO.ynDel}"/>
				</s:FormItem>
				<!--<s:Button id="button" label="Submit" click="button_clickHandler(event)"/>-->
			</s:Form>
		
		
		</s:VGroup>
	</s:Scroller>
</s:View>
