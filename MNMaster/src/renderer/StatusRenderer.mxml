<?xml version="1.0" encoding="utf-8"?>
<s:IconItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark"  labelField="dt"
					messageFunction="messagefunction" iconWidth="64" iconHeight="64">
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			
			import valueObjects.StatusVO;
			
			private function messagefunction(data:Object):Object
			{
				var rslt:String = "";
				if (data != null) {
					var mvo:StatusVO = data as StatusVO;
					rslt = "SMS:"+mvo.sms+" LMS:"+mvo.lms+" MMS:"+mvo.mms;
				}
				
				return rslt;
				
				
			} 
			
		]]>
	</fx:Script>
</s:IconItemRenderer>
